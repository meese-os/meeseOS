{
  "name": "@meese-os/cursor-effects",
  "version": "1.0.2",
  "description": "meeseOS Cursor Effects",
  "scripts": {
    "eslint": "eslint index.js",
    "test": "npm run eslint",
    "build": "webpack",
    "watch": "webpack --watch",
    "prepublishOnly": "npm run eslint && rd /s /q dist>nul 2>&1|echo.>nul && NODE_ENV=production npm run build"
  },
  "license": "BSD-3-Clause",
  "files": [
    "dist/",
    "metadata.json"
  ],
  "devDependencies": {
    "@meese-os/eslint-config": "workspace:*",
    "@babel/core": "^7.22.11",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/runtime": "^7.22.11",
    "babel-loader": "^9.1.3",
    "eslint": "^8.48.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "cursor-effects": "^1.0.12"
  },
  "meeseOS": {
    "type": "package"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 11
    },
    "extends": "@meese-os/eslint-config"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meeseOS/meeseOS.git",
    "directory": "frontend/cursor-effects"
  },
  "keywords": [
    "os",
    "osjs",
    "operating system",
    "webOS",
    "meeseOS",
    "cursor",
    "cursor effects"
  ]
}
