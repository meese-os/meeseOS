{
  "name": "@meese-os/google-api-provider",
  "version": "1.2.0",
  "description": "Google API Service Provider for meeseOS",
  "scripts": {
    "test": "npm run eslint",
    "eslint": "eslint *.js",
    "build": "webpack",
    "prepublishOnly": "npm run eslint && rm -rf ./dist/* && npm run build"
  },
  "license": "BSD-2-Clause",
  "files": [
    "dist/",
    "README.md"
  ],
  "devDependencies": {
    "@meese-os/eslint-config": "workspace:*",
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "babel-loader": "^10.0.0",
    "eslint": "^9.39.2",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {},
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 11
    },
    "extends": "@meese-os/eslint-config"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meese-os/meeseOS.git",
    "directory": "frontend/gui"
  },
  "keywords": [
    "os",
    "osjs",
    "operating system",
    "webOS",
    "meeseOS",
    "gapi",
    "google",
    "google-api"
  ]
}
